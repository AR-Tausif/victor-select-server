{
  "name": "victory-select-backend",
  "version": "1.0.0",
  "description": "the backend from victory select",
  "scripts": {
    "start": "nodemon -e js,graphql -x node src/index.js",
    "debug": "nodemon -e js,graphql -x node --inspect src/index.js",
    "dev": "nodemon -e js,graphql -x node --inspect=127.0.0.1:9230 src/index.js",
    "test": "jest",
    "deploy": "npx prisma1 deploy --env-file .env",
    "admin": "npx prisma1 admin",
    "realdata": "npx prisma1 admin --env-file .env.production",
    "realdeploy": "npx prisma1 deploy --env-file .env.production"
  },
  "dependencies": {
    "@sendgrid/mail": "^6.4.0",
    "@sentry/node": "5.14.2",
    "apollo-server-express": "^2.9.5",
    "authorizenet": "^1.0.7",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-session": "^1.16.2",
    "got": "^10.0.4",
    "graphql": "^14.5.8",
    "graphql-import": "^0.7.1",
    "helmet": "^3.21.1",
    "jsonwebtoken": "^8.5.1",
    "lodash.get": "^4.4.2",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.3.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prisma-binding": "^2.3.16",
    "prisma-client-lib": "^1.34.10",
    "reflect-metadata": "^0.1.13",
    "sha256": "^0.2.0",
    "shippo": "^1.5.0",
    "twilio": "^3.49.1"
  },
  "license": "MIT",
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "prisma1": "^1.34.11"
  }
}
